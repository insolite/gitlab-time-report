{
  "name": "gitlab-time-report",
  "version": "0.1.0",
  "description": "GitLab issue time stats report",
  "repository": "https://github.com/insolite/gitlab-time-report",
  "author": "Oleg Krasnikov",
  "readme": "README.rst",
  "private": true,
  "main": "lib",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "engines": {
    "node": ">=5.0",
    "npm": ">=3.3"
  },
  "dependencies": {
    "react": "15.6.1",
    "react-bootstrap-table": "3.5.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.3",
    "react-select": "1.0.0-rc.3",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "css-loader": "^0.28.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "style-loader": "^0.16.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "webpack && gulp sass && cp -R ./src/resources ./lib",
    "lib": "yarn clean && babel src -d lib",
    "prepublish": "yarn lib",
    "start": "webpack-dev-server"
  }
}
